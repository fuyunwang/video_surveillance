        <module>chuoyue-web</module>
    </modules>
    <parent>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-starter-parent</artifactId>
        <version>2.3.1.RELEASE</version>
        <relativePath/>
    </parent>
    <groupId>com.fuyunwang</groupId>
    <artifactId>chuoyue</artifactId>
    <version>1.0</version>
    <packaging>pom</packaging>

    <name>Chuoyue</name>
    <description>Graceful project created by springboot</description>

    <developers>
        <developer>
            <name>FuyunWang</name>
            <email>beautifulsoup@163.com</email>
        </developer>
    </developers>

    <properties>
        <chuoyue.version>1.0.0</chuoyue.version>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
        <java.version>1.8</java.version>
        <mybatis-plus.version>3.3.2</mybatis-plus.version>
        <druid.version>1.1.22</druid.version>
        <captcha.version>1.6.2</captcha.version>
        <jjwt.version>0.11.1</jjwt.version>
        <mapstruct.version>1.3.1.Final</mapstruct.version>
        <hutool.version>5.3.7</hutool.version>
        <swagger.version>2.9.2</swagger.version>
